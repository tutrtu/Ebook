@model EbookWEB.ViewModel.CombinedAuthViewModel

@{
    ViewData["Title"] = "Authentication";
}

<h2>Authentication</h2>

<div class="row">
    <div class="col-md-6">
        <h3>Login</h3>
        @await Html.PartialAsync("Login", Model.Login)
    </div>
    <div class="col-md-6">
        <h3>Register</h3>
        @await Html.PartialAsync("Register", Model.Register)
    </div>
</div>
